package variables;

public class variables {

	public static void main(String[] args) {

         byte test = 127;
         System.out.println(test);
     
         short test1 = 32767;
         System.out.println(test1);
         
         boolean test2 = false
         System.out.println(test2);
         
         double test3 = 2.31234567895555;
         System.out.println(test3);
	}

}
