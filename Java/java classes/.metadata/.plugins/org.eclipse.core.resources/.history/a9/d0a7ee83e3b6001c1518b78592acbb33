package variables;

public class variables {

	public static void main(String[] args) {
		 
         byte test = 127;
         System.out.println(test);
     
         short test1 = 32767;
         System.out.println(test1);
         
         double test2 = 2.31234567895555;
         System.out.println(test2);
         
         boolean test3 = false;
		 System.out.println(test3);
      
	}

}
