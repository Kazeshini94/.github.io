package helloworld;

public class Program {

	public static void main(String[] args) {
		
		int age = 27;      //Initialisierung
		
		int age2;          //Deklaration  (bekommt intern einen Basiswert zugeteilt)
		age2 = 29;         //Zuweisung

		System.out.println(age2);
		
		age2 = 30;
		
		System.out.println(age2);
		
	}

}
