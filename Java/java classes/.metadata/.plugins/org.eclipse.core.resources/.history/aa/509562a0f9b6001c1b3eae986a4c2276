package schleifen;

public class Schleifen {

	public static void main(String[] args) {
		
		int number = 1;
		
		while(number <= 5) {                 // Solange das Ergebnis true ist  wird der Code Block {} ausgefÃ¼hrt 
			System.out.println(number);      // while schleifen laufen solange bis das Ergebnis false ist und sind Boolesche Anfragen
			number = number + 1;
			
		}

	}

}
